<% if @url.any? %>
<% @url.each do |url| %>
    <h2> <%= url.name %> </h2>
    <h2> <%= url.link %> </h2>
  <% end %>
<% else %>
  No urls yet
<% end %>

<p>Add link</p>
<%= form_for @url.new do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :link %>
  <%= f.text_field :link %>
  <%= f.submit %>
<% end %>
